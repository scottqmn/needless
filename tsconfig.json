{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve"
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        "**/*.js",
        "custom.d.ts",
        "pages/_document.jsx",
        "pages/_app.jsx",
        "components/PostSlices/index.jsx",
        "components/PostSlices/Images/index.jsx",
        "components/PostSlices/RichText/index.jsx"
    ],
    "exclude": ["node_modules"]
}
